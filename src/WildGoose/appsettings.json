{
  "AllowedHosts": "*",
  "DbContext": {
    "TablePrefix": "wild_goose_",
    "UseUnderScoreCase": true,
    "ConnectionString": "User ID=postgres;Password=;Host=10.0.10.190;Port=5432;Database=;Pooling=true;"
  },
  "WildGoose": {
    "AddUserRoles": ["服务企业人员","企业管理员"]
  },
  "ApiName": "sample-wildgoose-api",
  "JwtBearer": {
    "Authority": "https://sample.abcd.cn/sts/",
    "RequireHttpsMetadata": false,
    "ValidateAudience": false,
    "ValidateIssuer": false
  },
  "Dapr": {
    "pubsub": "rabbitmq-pubsub"
  },
  "Identity": {
    "Password": {
      "RequireDigit": false,
      "RequireLowercase": false,
      "RequiredLength": 0,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": false,
      "RequiredUniqueChars": 1
    },
    "SignIn": {
      "RequireConfirmedEmail": false,
      "RequireConfirmedAccount": false,
      "RequireConfirmedPhoneNumber": false
    },
    "Lockout": {
      "DefaultLockoutTimeSpan": "00:05:00",
      "MaxFailedAccessAttempts": 5,
      "AllowedForNewUsers": false
    },
    "User": {
      "AllowedUserNameCharacters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-._@+",
      "RequireUniqueEmail": false
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Grafana.Loki", "Serilog.Enrichers.Environment", "Serilog.Enrichers.Thread", "Serilog.Sinks.Async", "Serilog.Expressions"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.EntityFrameworkCore": "Warning",
        "Orleans": "Error",
        "SocoDB": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputSocoDB": "{Timestamp:yyyy-MM-dd HH:mm:ss,fff} [{Level:u3}] [{SourceContext}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "http://10.0.10.190:3100",
          "labels": [
            {
              "key": "application",
              "value": "sample-wildgoose-api"
            }
          ],
          "propertiesAsLabels": ["classification", "level"]
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName"],
    "Properties": {
      "source": "backend"
    }
  }
}